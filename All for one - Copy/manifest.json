{
  "manifest_version": 3,
  "name": "Talabat Helper V4.5",
  "description": "Talabat order helper @Shalaby_Scott",
  "version": "4.0",
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}